{
  "name": "siyuan-plugin-2md",
  "version": "0.1.0",
  "type": "module",
  "description": "Batch convert the documents of Siyuan Notes into Markdown files, and support the conversion properties of FrontFormatter corresponding to Hexo, Hugo, Vitepress, Obsidian and other platforms",
  "repository": "terwer/siyuan-plugin-2md",
  "homepage": "https://github.com/terwer/siyuan-plugin-2md",
  "author": "terwer",
  "license": "GPL-3.0",
  "scripts": {
    "makeLink": "python scripts/make_dev_link.py",
    "serve": "vite",
    "dev": "vite build --watch",
    "build": "vite build",
    "start": "vite preview",
    "test": "vitest --watch",
    "syncVersion": "python scripts/version.py",
    "parseChangelog": "python scripts/parse_changelog.py",
    "prepareRelease": "pnpm syncVersion && pnpm parseChangelog",
    "package": "python scripts/package.py"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@terwer/eslint-config-custom": "^1.2.0",
    "@tsconfig/svelte": "^4.0.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^20.2.5",
    "fast-glob": "^3.2.12",
    "glob": "^7.2.3",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "siyuan": "^0.7.2",
    "stylus": "^0.59.0",
    "svelte": "^3.59.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-static-copy": "^0.15.0",
    "vite-plugin-zip-pack": "^1.0.5"
  },
  "dependencies": {
    "zhi-common": "^1.8.2",
    "zhi-device": "^1.0.1",
    "zhi-lib-base": "^0.1.0"
  },
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf"
